		  <IDSymbol name="idmWSE_ContextMenu" value="0x0002"/>
		  <IDSymbol name="idgWSE_ContextMenu_BuildDebugActions" value="0x014"/>
		  <IDSymbol name="idgWSE_ContextMenu_FileContextActions" value="0x0017"/>
